@article{hill_chemical_1995,
	title = {Chemical evolution of the {Magellanic} {Clouds}. {VI}. {Chemical} composition of nine {F} supergiants from different regions of the large {Magellanic} {Cloud}.},
	volume = {293},
	issn = {0004-6361},
	url = {https://ui.adsabs.harvard.edu/abs/1995A&A...293..347H/abstract},
	abstract = {In an effort to increase the available information on the chemical content of the Magellanic Clouds, we investigated nine F supergiants from the field of the LMC by high-resolution spectroscopy, in order to obtain the elemental abundance ratios for C, O, α-elements, Fe peak and heavy elements. The stars are widely distributed over the LMC. An LTE analysis has given the following results: 1. All investigated stars are metal deficient ([Fe/H] value covers the interval from -0.34dex to -0.14dex; mean value: -0.27dex). 2. The iron abundance appears to be surprisingly uniform, although the stars were chosen in different regions of the LMC. The star-to star scatter is within the observation uncertainty. 3. For all stars[C/Fe]\&lt;{\textasciitilde}0 but still exceeds the value observed in LMC HII regions by +0.2dex. 4. The [O/Fe] ratio is very similar to the one found for the Galactic supergiant Canopus, and the star-to-star scatter is small (σ=0.1dex) 5. Sodium does not seem to be enhanced (in average) in the LMC supergiants. 6. Among α-elements Si, S, Ca and Ti are enhanced in LMC supergiants, but Mg is slightly underabundant (with respect to Fe) when compared with solar value. 7. Heavy s- and r- process elements are overabundant by +0.3dex in average. This overabundance could be somewhat smaller for Eu, a pure r- process element.},
	language = {en},
	urldate = {2025-05-04},
	journal = {Astronomy and Astrophysics},
	author = {Hill, V. and Andrievsky, S. and Spite, M.},
	month = jan,
	year = {1995},
	pages = {347--359},
	file = {Full Text PDF:/Users/billyli/Zotero/storage/AZSPYQ57/Hill et al. - 1995 - Chemical evolution of the Magellanic Clouds. VI. Chemical composition of nine F supergiants from dif.pdf:application/pdf},
}

@article{hill_chemical_1997,
	title = {Chemical composition of six {K} supergiants in the {Small} {Magellanic} {Cloud}.},
	volume = {324},
	issn = {0004-6361},
	url = {https://ui.adsabs.harvard.edu/abs/1997A&A...324..435H/abstract},
	abstract = {A sample of six SMC cool stars (K supergiants) are analysed by high resolution spectroscopy in order to investigate their chemical content, which should reflect that of the current interstellar medium within the SMC. The abundance of Na, Al, α-elements, Fe peak and heavy elements are derived through an LTE analysis of the lines, compared to a Galactic "standard" (the well-studied K giant Arcturus) and discussed in terms of chemical evolution of the SMC. The following results were obtained: 1.) All the stars from this sample are metal deficient ([Fe/H] ranges from -0.59 to -0.89dex). The abundance varies little from star-to-star (0.10dex rms, comparable with the expected error due to uncertainties on the effective parameters of the stars), in agreement with the very small chemical composition variations among the H II regions within the SMC found by Pagel et al. (1978MNRAS.184..569P). 2.) Sodium is not strongly enhanced in these stars, and the star-to-star scatter is larger than expected. 3.) At variance with metal-poor stars in our Galaxy, the so-called α-elements do not seem to be enhanced. 4.) Nickel displays a noticeable depletion with respect to iron ([Ni/Fe]={\textasciitilde}-0.3dex), at variance with what is found in the SMC F supergiants and in Arcturus. 5.) Except for PMMR 144, the s and r process elements heavier than Ba are enhanced in all of our stars by ={\textasciitilde}+0.4dex.},
	language = {en},
	urldate = {2025-05-04},
	journal = {Astronomy and Astrophysics},
	author = {Hill, V.},
	month = aug,
	year = {1997},
	pages = {435--448},
	file = {Full Text PDF:/Users/billyli/Zotero/storage/UPHG6TNY/Hill - 1997 - Chemical composition of six K supergiants in the Small Magellanic Cloud..pdf:application/pdf},
}

@article{hill_carbon_1997,
	title = {Carbon, nitrogen, oxygen and lithium abundances of six cool supergiants in the {SMC}.},
	volume = {323},
	issn = {0004-6361},
	url = {https://ui.adsabs.harvard.edu/abs/1997A&A...323..461H/abstract},
	abstract = {Carbon, nitrogen and oxygen abundances were derived from high-resolution spectra of 6 cool supergiants of the Small Magellanic Cloud. Oxygen-to-iron ratios (mean value \&lt;[O/Fe]\&gt;={\textasciitilde}-0.18dex) are found to be similar to those found in young objects in the LMC and the Galaxy. This result is discussed in terms of chemical evolution. A mean deficiency of the carbon-to-iron ratio of \&lt;[C/Fe]\&gt;={\textasciitilde}-0.3dex and a nitrogen-to-iron ratio \&lt;[N/Fe]\&gt;={\textasciitilde}+0.22dex might imply a mixing signature, which is confirmed by the low {\textasciicircum}12{\textasciicircum}C/{\textasciicircum}13{\textasciicircum}C ratio measured in three stars. In terms of chemical evolution, carbon plus nitrogen abundances have to be considered: in our sample, a mean value of \&lt;[(C+N)/Fe]\&gt;={\textasciitilde}-0.15dex indicates a slight deficiency in carbon plus nitrogen with respect to the Sun, similar to the deficiencies found in Galactic supergiants and Orion. Lithium has been detected in all the program stars, reaching ɛ(Li)=0.6dex for two stars.},
	language = {en},
	urldate = {2025-05-04},
	journal = {Astronomy and Astrophysics},
	author = {Hill, V. and Barbuy, B. and Spite, M.},
	month = jul,
	year = {1997},
	pages = {461--468},
	file = {Full Text PDF:/Users/billyli/Zotero/storage/LY55ZL6B/Hill et al. - 1997 - Carbon, nitrogen, oxygen and lithium abundances of six cool supergiants in the SMC..pdf:application/pdf},
}


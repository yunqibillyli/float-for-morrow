	pages = {461--473},
	file = {Full Text PDF:/Users/billyli/Zotero/storage/ND4QL4JU/Dufour et al. - 1982 - The carbon abundance in the Magellanic Clouds from IUE observations..pdf:application/pdf},
}

@article{pagel_survey_1978,

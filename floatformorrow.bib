}

@article{lah_emission_2024,
	title = {Emission line velocity, metallicity, and extinction maps of the {Small} {Magellanic} {Cloud}},
	volume = {535},
	issn = {0035-8711},
	url = {https://ui.adsabs.harvard.edu/abs/2024MNRAS.535.2598L/abstract},
	doi = {10.1093/mnras/stae2534},
	abstract = {Optical emission lines across the Small Magellanic Cloud (SMC) have been measured from multiple fields using the Australian National University 2.3m telescope with the wide-field spectrograph. Interpolated maps of the gas-phase metallicity, extinction, H {\textless}inline-formula{\textgreater}{\textless}tex-math id="TM0001" notation="LaTeX"{\textgreater}\${\textbackslash}alpha\${\textless}/tex-math{\textgreater}{\textless}/inline-formula{\textgreater} radial velocity, and H {\textless}inline-formula{\textgreater}{\textless}tex-math id="TM0002" notation="LaTeX"{\textgreater}\${\textbackslash}alpha\${\textless}/tex-math{\textgreater}{\textless}/inline-formula{\textgreater} velocity dispersion have been made from these measurements. There is a metallicity gradient from the centre to the north of the galaxy of {\textless}inline-formula{\textgreater}{\textless}tex-math id="TM0003" notation="LaTeX"{\textgreater}\${\textbackslash}sim\${\textless}/tex-math{\textgreater}{\textless}/inline-formula{\textgreater}-0.095 dex kpc{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater} with a shallower metallicity gradient from the centre to the south of the galaxy of {\textless}inline-formula{\textgreater}{\textless}tex-math id="TM0004" notation="LaTeX"{\textgreater}\${\textbackslash}sim\${\textless}/tex-math{\textgreater}{\textless}/inline-formula{\textgreater}-0.013 dex kpc{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater}. There is an extinction gradient of {\textless}inline-formula{\textgreater}{\textless}tex-math id="TM0005" notation="LaTeX"{\textgreater}\${\textbackslash}sim\${\textless}/tex-math{\textgreater}{\textless}/inline-formula{\textgreater}-0.086 E(B - V)/kpc from the centre going north and shallower going from the centre to the south of {\textless}inline-formula{\textgreater}{\textless}tex-math id="TM0006" notation="LaTeX"{\textgreater}\${\textbackslash}sim\${\textless}/tex-math{\textgreater}{\textless}/inline-formula{\textgreater}-0.0089 E(B - V)/kpc. The SMC eastern arm has lower extinction than the main body. The radial velocity of the gas from the H {\textless}inline-formula{\textgreater}{\textless}tex-math id="TM0007" notation="LaTeX"{\textgreater}\${\textbackslash}alpha\${\textless}/tex-math{\textgreater}{\textless}/inline-formula{\textgreater} line and the H I line have been compared across the SMC. In general there is good agreement between the two measurements, though there are a few notable exceptions. Both show a region that has different radial velocity to the bulk motion of the SMC in the southern western corner by at least 16 km s{\textless}inline-formula{\textgreater}{\textless}tex-math id="TM0008" notation="LaTeX"{\textgreater}\${\textasciicircum}\{-1\}\${\textless}/tex-math{\textgreater}{\textless}/inline-formula{\textgreater}. The velocity dispersion from H {\textless}inline-formula{\textgreater}{\textless}tex-math id="TM0009" notation="LaTeX"{\textgreater}\${\textbackslash}alpha\${\textless}/tex-math{\textgreater}{\textless}/inline-formula{\textgreater} and H I across the SMC have also been compared, with the H {\textless}inline-formula{\textgreater}{\textless}tex-math id="TM0010" notation="LaTeX"{\textgreater}\${\textbackslash}alpha\${\textless}/tex-math{\textgreater}{\textless}/inline-formula{\textgreater} velocity dispersion usually the higher of the two. The eastern arm of the SMC generally has lower velocity dispersion than the SMC's main body. These measurements enable a detailed examination of the SMC, highlighting its nature as a disrupted satellite galaxy.},
	language = {en},
	number = {3},
	urldate = {2025-05-24},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Lah, Philip and Colless, Matthew and D'Eugenio, Francesco and Groves, Brent and Gelfand, Joseph D.},
	month = dec,
	year = {2024},
	pages = {2598--2617},
	file = {Submitted Version:/Users/billyli/Zotero/storage/XC836FER/Lah et al. - 2024 - Emission line velocity, metallicity, and extinction maps of the Small Magellanic Cloud.pdf:application/pdf},
}


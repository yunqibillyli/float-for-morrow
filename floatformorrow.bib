@article{bowen_absorption-line_2020,
	title = {Absorption-line {Abundances} in the {SMC}-like {Galaxy} {UGC} 5282: {Evidence} of {ISM} {Dilution} from {Inflows} on {Kiloparsec} {Scales}*},
